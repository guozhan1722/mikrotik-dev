config wifi-device  radio0
	option type     mac80211
	option country	US
	option channel  149
	option path	'platform/ar934x_wmac'
	option distance 5000
	option hwmode	11a
	option htmode	HT40+
	option diversity 0
	option txantenna 1
	option rxantenna 1

	list ht_capab	SHORT-GI-40
	list ht_capab	TX-STBC
	list ht_capab	RX-STBC1
	list ht_capab	DSSS_CCK-40

config wifi-iface
	option device   radio0
	option network  lan
	option mode     sta
	option wds	1
	option ssid     Teletics_ZipLine_Ethernet_Extr
	option encryption psk+aes
	option key TVop36wixOKwaJJVtxJxAFUwzZhANCXt6cSRi3JMSEAJ8PA34DZ5slBXT2YH2YY

